{
    "name": "glesys/butler-service",
    "type": "library",
    "description": "Web service library based on Laravel",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "bugsnag/bugsnag-laravel": "^2.18",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "glesys/butler-auth": "^2.0",
        "glesys/butler-graphql": "3.1",
        "glesys/butler-guru": "^0.4.0",
        "graylog2/gelf-php": "^1.6",
        "laravel/framework": "^7.5",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.3",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Butler\\Service\\ServiceProvider"
            ],
            "dont-discover": [
                "glesys/butler-auth",
                "glesys/butler-graphql",
                "glesys/butler-guru"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Butler\\Service\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Butler\\Service\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
