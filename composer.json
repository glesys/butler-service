{
    "name": "glesys/butler-service",
    "type": "library",
    "description": "Web service library based on Laravel",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "bugsnag/bugsnag-laravel": "^2.18",
        "doctrine/dbal": "^2.10 || ^3.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "glesys/butler-audit": "^0.3",
        "glesys/butler-auth": "^4.0",
        "glesys/butler-graphql": "^5.1",
        "glesys/butler-guru": "^0.7",
        "graylog2/gelf-php": "^1.6",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.33",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.6",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.12",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "glesys/butler-service"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Butler\\Service\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Butler\\Service\\Tests\\": "tests/",
            "App\\": "vendor/orchestra/testbench-core/laravel/app"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
