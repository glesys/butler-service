{
    "name": "glesys/butler-service",
    "type": "library",
    "description": "Web service library based on Laravel",
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "bugsnag/bugsnag-laravel": "^2.18",
        "glesys/butler-audit": "^0.6",
        "glesys/butler-auth": "^5.0",
        "glesys/butler-graphql": "^9.0",
        "glesys/butler-health": "^0.2",
        "graylog2/gelf-php": "^1.6",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.35",
        "laravel/octane": "^1.0",
        "laravel/tinker": "^2.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.12",
        "graham-campbell/testbench": "^5.6",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "glesys/butler-service"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Butler\\Service\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Butler\\Service\\Tests\\": "tests/",
            "App\\": "vendor/orchestra/testbench-core/laravel/app"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
